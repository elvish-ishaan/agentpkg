# Server Configuration
NODE_ENV=development
PORT=4000
API_BASE_URL=http://localhost:4000

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/agentpkg

# GCP Storage Configuration
GCP_PROJECT_ID=your-project-id
GCP_BUCKET_NAME=agentpkg-registry
GCP_KEY_FILE=./gcp-service-account.json

# Security Configuration
# Generate a random 32+ character string for production
API_TOKEN_SECRET=your-secret-key-min-32-chars-change-in-production
BCRYPT_ROUNDS=10

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_AUTH_MAX=5
RATE_LIMIT_PUBLISH_MAX=10
